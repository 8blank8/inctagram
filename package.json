{
  "name": "inctagram",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:inctagram:dev": "nest start --watch",
    "build:inctagram": "nest build inctagram",
    "start:inctagram": "node dist/apps/inctagram/main",
    "start:files:dev": "nest start files --watch",
    "build:files": "nest build files",
    "start:files": "node dist/apps/files/main",
    "start:debug": "nest start --debug --watch",
    "migrate:postgres": "dotenv -e .env.sample -- npx prisma migrate deploy",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --testPathIgnorePatterns=\"/*.e2e.spec.ts$\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand --testRegex=\"^.*e2e.*.spec.ts$\"",
    "prisma:generate": "yarn prisma generate",
    "prisma:reset": "yarn prisma migrate reset",
    "prisma:deploy": "yarn prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "pre-commit": {
    "run": "lint",
    "silent": false
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.451.0",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/microservices": "^10.2.7",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/throttler": "^5.0.0",
    "@prisma/client": "^5.5.2",
    "axios": "^1.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto": "^1.0.1",
    "passport": "^0.6.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "unique-username-generator": "^1.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.13",
    "@types/passport-github": "^1.1.11",
    "@types/passport-google-oauth2": "^0.1.7",
    "@types/passport-jwt": "^3.0.11",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "2.0.4",
    "multer": "^1.4.5-lts.1",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.0",
    "prisma": "^5.4.2",
    "sharp": "^0.32.6",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/main(|/.*)$": "<rootDir>/apps/inctagram/src/$1",
      "^@app/files(|/.*)$": "<rootDir>/apps/files/src/$1",
      "^@app/user": "<rootDir>/libs/user/src",
      "^@app/common": "<rootDir>/libs/common",
      "^@app/db": "<rootDir>/libs/db/src",
      "^@app/auth(|/.*)$": "<rootDir>/libs/auth/src/$1"
    }
  }
}
